    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "astro"
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist/", "node_modules"]
}
